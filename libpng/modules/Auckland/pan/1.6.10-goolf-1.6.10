#%Module

proc ModulesHelp { } {
    puts stderr {   libpng is the official PNG reference library - Homepage: http://www.libpng.org/pub/png/libpng.html
    }
}

module-whatis {Description: libpng is the official PNG reference library - Homepage: http://www.libpng.org/pub/png/libpng.html}

set root    /share/easybuild/RHEL6.3/westmere/software/libpng/1.6.10-goolf-1.6.10

conflict    libpng

if { ![is-loaded goolf/1.6.10] } {
    module load goolf/1.6.10
}

if { ![is-loaded zlib/1.2.8-goolf-1.6.10] } {
    module load zlib/1.2.8-goolf-1.6.10
}

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin
prepend-path	PKG_CONFIG_PATH		$root/lib/pkgconfig

setenv	EBROOTLIBPNG		"$root"
setenv	EBVERSIONLIBPNG		"1.6.10"
setenv	EBDEVELLIBPNG		"$root/easybuild/libpng-1.6.10-goolf-1.6.10-easybuild-devel"


